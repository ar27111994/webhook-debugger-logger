{
  "urlCount": 3,
  "retentionHours": 24,
  "maxPayloadSize": 10485760,
  "enableJSONParsing": true,
  "maskSensitiveData": true,
  "rateLimitPerMinute": 60,
  "authKey": "my-secret-key-123",
  "defaultResponseCode": 200,
  "defaultResponseBody": "{\"status\": \"received\", \"success\": true}",
  "responseDelayMs": 0,
  "forwardUrl": "https://your-server.com/ingest",
  "forwardHeaders": true,
  "jsonSchema": "{\n  \"type\": \"object\",\n  \"required\": [\"userId\"],\n  \"properties\": {\n    \"userId\": { \"type\": \"string\" }\n  }\n}",
  "customScript": "event.body = JSON.parse(event.body);\nevent.headers['x-custom'] = 'modified';",
  "testAndExit": false
}
