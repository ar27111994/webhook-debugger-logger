{
  "actorOutputSchemaVersion": 1,
  "title": "Webhook Debugger Output",
  "properties": {
    "dataset": {
      "type": "string",
      "title": "Captured Events (Live)",
      "template": "{{links.apiDefaultDatasetUrl}}/items"
    },
    "info": {
      "type": "string",
      "title": "Active Webhooks Info",
      "template": "{{links.containerRunUrl}}/info"
    },
    "webhooks": {
      "type": "string",
      "title": "Storage State",
      "template": "{{links.apiDefaultKeyValueStoreUrl}}/keys"
    }
  },
  "views": {
    "overview": {
      "title": "Overview",
      "transformation": {
        "fields": [
          "timestamp",
          "requestId",
          "signatureValid",
          "webhookId",
          "method",
          "statusCode",
          "contentType",
          "size",
          "processingTime",
          "remoteIp",
          "signatureProvider"
        ],
        "desc": true
      },
      "display": {
        "component": "table"
      }
    },
    "details": {
      "title": "Full Payloads",
      "transformation": {
        "fields": [
          "timestamp",
          "requestId",
          "signatureValid",
          "method",
          "statusCode",
          "headers",
          "body",
          "responseBody",
          "responseHeaders",
          "signatureProvider",
          "signatureError"
        ],
        "desc": true
      },
      "display": {
        "component": "table"
      }
    }
  }
}
