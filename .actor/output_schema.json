{
  "actorOutputSchemaVersion": 1,
  "title": "Webhook Debugger Output",
  "properties": {
    "dataset": {
      "type": "string",
      "title": "Captured Events (Live)",
      "template": "{{links.apiDefaultDatasetUrl}}/items"
    },
    "dataset_csv": {
      "type": "string",
      "title": "Download logs (CSV)",
      "template": "{{links.apiDefaultDatasetUrl}}/items?format=csv&clean=true"
    },
    "dataset_json": {
      "type": "string",
      "title": "Download logs (JSON)",
      "template": "{{links.apiDefaultDatasetUrl}}/items?format=json&clean=true"
    },
    "info": {
      "type": "string",
      "title": "Active Webhooks Info",
      "template": "{{links.containerRunUrl}}/info"
    },
    "webhooks": {
      "type": "string",
      "title": "Storage State",
      "template": "{{links.apiDefaultKeyValueStoreUrl}}/keys"
    }
  },
  "views": {
    "overview": {
      "title": "Overview",
      "transformation": {
        "fields": [
          "timestamp",
          "webhookId",
          "method",
          "statusCode",
          "contentType",
          "size",
          "processingTime",
          "remoteIp"
        ],
        "desc": true
      },
      "display": {
        "component": "table"
      }
    },
    "details": {
      "title": "Full Payloads",
      "transformation": {
        "fields": ["timestamp", "method", "statusCode", "headers", "body"],
        "desc": true
      },
      "display": {
        "component": "table"
      }
    }
  }
}
