{
  "name": "webhook-debugger-logger",
  "version": "3.1.3",
  "type": "module",
  "description": "Generate temporary webhook URLs and log all incoming requests with full details.",
  "main": "src/main.js",
  "bin": {
    "webhook-debugger-logger": "src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ar27111994/webhook-debugger-logger.git"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "node --experimental-vm-modules --max-old-space-size=8192 node_modules/jest/bin/jest.js --runInBand --detectOpenHandles --forceExit",
    "test:stress": "NODE_OPTIONS=\"$NODE_OPTIONS --expose-gc\" npm run test -- tests/e2e/stress.test.js",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "sync:version": "node scripts/sync-version.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.4.3-r.3",
    "ajv": "^8.17.1",
    "apify": "^3.5.3",
    "axios": "^1.13.2",
    "bottleneck": "^2.19.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "ipaddr.js": "^2.3.0",
    "nanoid": "^5.1.6",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0"
  },
  "keywords": [
    "apify",
    "webhook",
    "debugger",
    "logger",
    "serverless",
    "actor"
  ],
  "author": "Apify",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ar27111994/webhook-debugger-logger/issues"
  },
  "homepage": "https://github.com/ar27111994/webhook-debugger-logger#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@apify/actor-memory-expression": "^0.1.6",
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@types/supertest": "^6.0.3",
    "eslint": "^10.0.0",
    "eslint-plugin-sonarjs": "^3.0.7",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "ts-api-utils": "^2.4.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
