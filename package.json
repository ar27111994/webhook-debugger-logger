{
  "name": "webhook-debugger-logger",
  "version": "2.8.3",
  "type": "module",
  "description": "Generate temporary webhook URLs and log all incoming requests with full details.",
  "main": "src/main.js",
  "bin": {
    "webhook-debugger-logger": "src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "apify": "^3.5.3",
    "axios": "^1.13.2",
    "body-parser": "^2.2.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "eventsource": "^4.1.0",
    "express": "^5.2.1",
    "ipaddr.js": "^2.3.0",
    "nanoid": "^5.1.6",
    "qs": "^6.14.1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.2.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "transform": {},
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
